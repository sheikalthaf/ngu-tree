{
  "name": "ngu-tree",
  "version": "0.1.0",
  "description": "Angular Universal tree",
  "scripts": {
    "transpile": "ngc ",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ngutree.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngutree.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "@angular/compiler": "^4.2.4",
    "@angular/compiler-cli": "^4.2.4",
    "rollup": "^0.43.0",
    "typescript": "^2.3.4",
    "uglify-js": "^3.0.19"
  },
  "dependencies": {
    "@angular/common": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "rxjs": "^5.5.2"
  },
  "peerDependencies": {
    "@angular/common": ">=4.0.0",
    "@angular/core": ">=4.0.0",
    "@angular/platform-browser": ">=4.0.0"
  }
}
